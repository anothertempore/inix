{
    "compilerOptions": {
      "baseUrl": "./",
      "outDir": "./dist/",
      "lib": ["es2019"],
      "target": "es2019",
      "module": "commonjs",
      "sourceMap": true,
      "noImplicitAny": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "removeComments": true,
      "allowJs": true,
      "skipLibCheck": true,
      "declaration": true
    }
  }
  